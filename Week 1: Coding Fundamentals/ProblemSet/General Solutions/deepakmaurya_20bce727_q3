#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    int a[n];
    for(int i=1;i<=n-1;i++){
        cin>>a[i-1];
    }
    sort(a,a+n);
    for(int i=1;i<=n;i++){
        if(i!=a[i-1]){
            cout<<i;
            break;
        }
    }
    return 0;
}
